                                                    if(data_words.length > 0 || data_kanjis.length > 0 ) {
                                                %>    

                                                <!--***********tuvung*************-->
                                                <div id="tuvung" class="tab-pane fade in active">
                                                    <% if( data_words.length > 0 ){ %>

                                                      <% data_words.forEach(function(index, num){ %>
